{
    "manifest_version": 2,
    "name": "Cookie Editor Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "cookies",
      "storage",
      "https://*/",
      "http://*/"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        
      }
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      
    }
  }
  